{
    "name": "maileryio/mailery-menu",
    "type": "library",
    "description": "Mailery Menu module",
    "keywords": [
        "mail",
        "mailer",
        "menu",
        "yii"
    ],
    "homepage": "https://github.com/maileryio/mailery-menu",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/maileryio/mailery-menu",
        "issues": "https://github.com/maileryio/mailery-menu/issues",
        "wiki": "https://github.com/maileryio/mailery-menu/wiki"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "yiisoft/html": "^3.0@dev",
        "yiisoft/widget": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phan/phan": "^2.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Mailery\\Menu\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailery\\Menu\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix --diff --dry-run",
        "fix": "php-cs-fixer fix",
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
